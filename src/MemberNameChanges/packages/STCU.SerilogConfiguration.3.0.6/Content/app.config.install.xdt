    <add key="serilog:using" value="Serilog.Sinks.Seq" xdt:Transform="InsertIfMissing" xdt:Locator="Match(key)" />
    <add key="serilog:write-to:Seq.serverUrl" value="https://seqlogtest" xdt:Transform="InsertIfMissing" xdt:Locator="Match(key)" />
    <add key="Application.Name" value="InsertApplicationName" xdt:Transform="InsertIfMissing" xdt:Locator="Match(key)" />
 </appSettings>
</configuration>
